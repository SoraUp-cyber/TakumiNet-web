{
  "version": 2,
  "builds": [
    {
      "src": "pages/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "js/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "lang/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "music/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "pdf/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/pages/$1" }
  ]
}
